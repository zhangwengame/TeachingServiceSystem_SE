<!doctype html>
<html>
<head>
<% include ../head.html %>
</head>
<body>
<!-- 上方选择条 navbar-top-->
<% include ../navbar.html %>
<!-- 侧边框 -->
<%if(type==0) {%>
<% include slidebar_student.html%>
<% }%>
<%if(type==1) {%>
<% include slidebar_teacher.html%>
<% }%>
<%if(type==2) {%>
<% include arrange_slidebar.html%>
<% }%>
<script type="text/javascript">
   function setNavActive(){
       var active=document.getElementById('arrange');
           active.className='active';
    }

   function setSlideActive(){
        var slide = document.getElementById('arrange_by_sa');
            slide.className='active';
   }

     setNavActive();
     setSlideActive();
</script>

<!-- 主要内容 -->
<div class="main-content">
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="area-top clearfix">
        <div class="pull-left header">
          <h3 class="title"><i class="icon-search"></i>自动排课</h3>
        </div>
      </div>
    </div>
  </div>
  <!-- 信息填写 -->
  <div class="span12">
    <div class="box">
      <form name="input" action="/arrange/arrange_by_sa" method="post">
        <div class="box-content padded">
          <div class="row-fluid">
            <button type="submit" class="btn btn-blue">开始排课</button>
          </div>
        </div>
      </form>
      </div>
      <form action="/arrange_by_sa" role="form" method="post">
        <p><span><%= saresult %></span></p>
      </form>
    </div>
   </div>
</div>

</body>
</html>
